-- DO NOT EDIT ANYTHING BELOW --

-- VARIABLES --

local plr = game.Players.LocalPlayer
local gui = plr.PlayerGui
local uis = game:GetService("UserInputService")
local toPrint

-- CODE --

local ECOSGUI = gui:WaitForChild("ECOS")	
local INVISIBLE = ECOSGUI:WaitForChild("MAIN")
local ECOS = INVISIBLE:WaitForChild("ECOS")
local SCROLL = ECOS:WaitForChild("COMMANDS")
local NEARINPUT = ECOS:WaitForChild("TXTNEARINPUTERDECORATE")
local EGOSVER = ECOSGUI:WaitForChild("EGOSVER")
local EGOSEDIT = ECOSGUI:WaitForChild("EGOSEDIT")

EGOSVER.Value = "25"
EGOSEDIT.Value = "Classic"

local function NewLine(toPrint)
	local LINE = Instance.new("TextLabel")
	LINE.Parent = NEARINPUT
	LINE.Size = UDim2.new(1, 0, 1, 0)
	LINE.BackgroundTransparency = 1
	LINE.Name = "LINEEGOSFROM"
	LINE.Text = toPrint
	LINE.TextColor3 = Color3.fromRGB(193, 193, 193)
	LINE.TextScaled = true
	LINE.Font = Enum.Font.Arcade
	LINE.TextXAlignment = Enum.TextXAlignment.Left
	LINE.TextTransparency = 1
end

toPrint = "- CREATING EGOS 25 FRAME"
NewLine(toPrint)
loadstring(game:HttpGet("https://raw.githubusercontent.com/Meni9797/ECOS/refs/heads/main/CreateEGOS25"))() -- Create EGOS
wait(1)
local MAIN = INVISIBLE:WaitForChild("EGOS")
toPrint = "- FRAME WAS CREATED SUCESSFULLY"
NewLine(toPrint)
toPrint = "- EXPLOIT GRAPHIC OPERATING SYSTEM 25 HAS BEEN SUCCESSFULLY CREATED"
NewLine(toPrint)
toPrint = "- EGOS 25"
NewLine(toPrint)
toPrint = "- BASED ON WINDOWS 95 AND WINDOWS 98"
NewLine(toPrint)
toPrint = "- 2025 BY MENI9797"
NewLine(toPrint)

local EGOS = INVISIBLE:WaitForChild("EGOS")
local TASKBAR = EGOS:WaitForChild("Taskbar")
local STARTBUTTON = TASKBAR:WaitForChild("StartButton")
local STARTMENU = EGOS:WaitForChild("StartMenu")
local PROGRAMSBUTTON = STARTMENU:WaitForChild("ProgramsButton")
local RUNBUTTON = STARTMENU:WaitForChild("RunButton")
local HELPBUTTON = STARTMENU:WaitForChild("HelpButton")
local PROGRAMSMENU = EGOS:WaitForChild("ProgramsMenu")
local EXPLORERBUTTON = PROGRAMSMENU:WaitForChild("ExplorerButton")
local RC7BUTTON = PROGRAMSMENU:WaitForChild("RC7Button")

STARTBUTTON.MouseButton1Click:Connect(function()
	if STARTMENU.Visible == true then
		STARTMENU.Visible = false
	else
		STARTMENU.Visible = true
	end
end)

PROGRAMSBUTTON.MouseEnter:Connect(function()
	PROGRAMSMENU.Visible = true
end)

RUNBUTTON.MouseEnter:Connect(function()
	PROGRAMSMENU.Visible = false
end)

HELPBUTTON.MouseEnter:Connect(function()
	PROGRAMSMENU.Visible = false
end)

EXPLORERBUTTON.MouseButton1Click:Connect(function()
	local expl = FILES:FindFirstChild("fileexplorer.SCR")
	if expl then
		loadstring(game:HttpGet(expl.Value))()
		PROGRAMSMENU.Visible = false
		STARTMENU.Visible = false
	end
end)

RC7BUTTON.MouseButton1Click:Connect(function()
	local rc7 = FILES:FindFirstChild("rc7.SCR")
	if rc7 then
		loadstring(game:HttpGet(rc7.Value))()
		PROGRAMSMENU.Visible = false
		STARTMENU.Visible = false
	end
end)
